plotElipseGauss<- function( rx, ry ){
r<- seq(-pi, pi, by = 0.001)
Xa<-rx*cos(r)
Ya<-rx*sin(r)
line1=cbind(Xa,Ya);
lineAll<-rbind(line1)
xxlim<-10
par(new = TRUE)
plot(lineAll,type="l", xlim=c(-xxlim,xxlim),ylim=c(-xxlim,xxlim),col="black",axes=FALSE, ann=FALSE, lwd=2)
x_0<- seq(-rx, rx, by = 0.001)
par(new = TRUE)
plot(x_0,0*x_0^2,type="l",xlim=c(-xxlim,xxlim),ylim=c(-xxlim,xxlim),col="black", ann=FALSE, lwd=2)
}
